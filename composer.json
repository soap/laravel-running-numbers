{
    "name": "soap/laravel-running-numbers",
    "description": "This is my package laravel-running-numbers",
    "keywords": [
        "Prasit Gebsaap",
        "laravel",
        "laravel-running-numbers"
    ],
    "homepage": "https://github.com/soap/laravel-running-numbers",
    "license": "MIT",
    "authors": [
        {
            "name": "Prasit Gebsaap",
            "email": "prasit.gebsaap@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0||^2.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.10.0||^8.1.1||^9.0",
        "orchestra/testbench": "^8.22.0||^9.0.0||^10.0",
        "pestphp/pest": "^2.34||^3.0",
        "pestphp/pest-plugin-arch": "^2.7||^3.0",
        "pestphp/pest-plugin-laravel": "^2.3||^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Soap\\Laravel\\RunningNumbers\\": "src/",
            "Soap\\Laravel\\RunningNumbers\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soap\\Laravel\\RunningNumbers\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-running-numbers --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soap\\Laravel\\RunningNumbers\\RunningNumberServiceProvider"
            ],
            "aliases": {
                "RunningNumber": "Soap\\Laravel\\RunningNumbers\\Facades\\RunningNumber"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
